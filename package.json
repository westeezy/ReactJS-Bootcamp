{
  "name": "ReactJS-Bootcamp",
  "version": "0.0.1",
  "description": "",
  "repository": "",
  "config": {
    "buildDir": "./build",
    "buildDirTests": "./build_tests",
    "devHost": "0.0.0.0",
    "devPort": 8001
  },
  "scripts": {
    "build": "NODE_ENV=production npm run webpack",
    "clean": "./node_modules/.bin/rimraf $npm_package_config_buildDir && mkdir $npm_package_config_buildDir",
    "lint": "eslint --ext .js --ext .jsx ./app ./webpack && echo No linting errors.",
    "prebuild": "npm run clean",
    "prestarts": "npm install",
    "pretest": "npm install && npm run lint",
    "pretest-travis": "npm install && npm run lint",
    "start": "NODE_ENV=development node dev-server ./webpack/config",
    "start-windows": "set NODE_ENV=development node dev-server ./webpack/config",
    "test": "NODE_ENV=test karma start",
    "test-travis": "NODE_ENV=test karma start --single-run",
    "webpack": "./node_modules/.bin/webpack --colors --progress --config ./webpack/config",
    "heroku-build": "npm run build && ./node_modules/http-server/bin/http-server build"
  },
  "dependencies": {
    "flux": "^2.1.1",
    "http-server": "^0.9.0",
    "lodash": "^4.6.1",
    "normalize.css": "^3.0.3",
    "page": "^1.6.4",
    "react": "^0.14.7",
    "react-dom": "^0.14.7"
  },
  "devDependencies": {
    "autoprefixer": "^6.3.3",
    "babel-core": "^6.7.0",
    "babel-loader": "^6.2.4",
    "babel-plugin-react-transform": "^2.0.2",
    "babel-plugin-rewire": "^1.0.0-rc-1",
    "babel-plugin-transform-runtime": "^6.6.0",
    "babel-polyfill": "^6.6.1",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-0": "^6.5.0",
    "babel-runtime": "^6.6.1",
    "chai": "^3.5.0",
    "css-loader": "^0.23.1",
    "eslint": "^2.4.0",
    "eslint-config-airbnb": "^6.1.0",
    "eslint-plugin-react": "^4.2.1",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.8.5",
    "font-awesome": "^4.5.0",
    "glob": "^7.0.3",
    "html-loader": "^0.4.3",
    "isparta": "^4.0.0",
    "isparta-loader": "^2.0.0",
    "json-loader": "^0.5.4",
    "karma": "^0.13.22",
    "karma-chrome-launcher": "^0.2.2",
    "karma-cli": "0.1.2",
    "karma-coverage": "^0.5.5",
    "karma-mocha": "^0.2.2",
    "karma-mocha-reporter": "^2.0.0",
    "karma-phantomjs-launcher": "^1.0.0",
    "karma-sinon": "^1.0.4",
    "karma-source-map-support": "^1.1.0",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-webpack": "^1.7.0",
    "mocha": "^2.4.5",
    "mocha-loader": "^0.7.1",
    "node-libs-browser": "^1.0.0",
    "node-sass": "^3.4.2",
    "opn": "^4.0.1",
    "phantomjs-prebuilt": "^2.1.5",
    "postcss-loader": "^0.8.1",
    "react-addons-test-utils": "^0.14.7",
    "react-transform-hmr": "^1.0.4",
    "rimraf": "^2.5.2",
    "sass-loader": "^3.1.2",
    "sinon": "^1.17.3",
    "source-map-support": "^0.4.0",
    "style-loader": "^0.13.0",
    "template-html-loader": "0.0.3",
    "url-loader": "^0.5.7",
    "webpack": "^1.12.14",
    "webpack-dev-server": "^1.14.1"
  },
  "engines": {
    "node": ">=0.10.0"
  }
}
